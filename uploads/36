function correctCollectionsHeight()
{
	$(".collections .row").each(function() {
		var me = $(this);
		var maxHeight = 0;
		me.find(".collection").each(function() {
			if ($(this).height() > maxHeight)
			{
				maxHeight = $(this).height();
			}
		});
		me.find(".collection").height(maxHeight);
	});
}
</script>



